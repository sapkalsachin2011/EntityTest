{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "build",
			"command": "dotnet",
			"type": "process",
			"args": [
				"build",
				"${workspaceFolder}/EntityTestApi/EntityTestApi.csproj",
				"/property:GenerateFullPaths=true",
				"/consoleloggerparameters:NoSummary;ForceNoAlign"
			],
			"problemMatcher": "$msCompile"
		},
		{
			"label": "publish",
			"command": "dotnet",
			"type": "process",
			"args": [
				"publish",
				"${workspaceFolder}/EntityTestApi/EntityTestApi.csproj",
				"/property:GenerateFullPaths=true",
				"/consoleloggerparameters:NoSummary;ForceNoAlign"
			],
			"problemMatcher": "$msCompile"
		},
		{
			"label": "watch",
			"command": "dotnet",
			"type": "process",
			"args": [
				"watch",
				"run",
				"--project",
				"${workspaceFolder}/EntityTestApi/EntityTestApi.csproj"
			],
			"problemMatcher": "$msCompile"
		},
		{
			"label": "add redis nuget",
			"type": "shell",
			"command": "dotnet",
			"args": [
				"add",
				"EntityTestApi/EntityTestApi.csproj",
				"package",
				"Microsoft.Extensions.Caching.StackExchangeRedis"
			]
		},
		{
			"label": "build",
			"type": "shell",
			"command": "dotnet",
			"args": [
				"build",
				"EntityTestApi/EntityTestApi.csproj",
				"/property:GenerateFullPaths=true",
				"/consoleloggerparameters:NoSummary;ForceNoAlign"
			]
		}
	]
}